#include <stdio.h>
int main(int argc, char const *argv[])
{
    int n,c,t,a[120],i,j;
    scanf("%d",&t);
    for(i=0;i<t;i++)
    {
        scanf("%d %d",&n,&c);
        for(j=0;j<n;j++)
        {
            scanf("%d",&a[j]);
        }
        for(j=0;j<n;j++)
        {
            c=c-a[j];
        }
        if(c >= 0)
        {
            printf("Yes\n");
        }
        else
            printf("No\n");
    }
    return 0;
}
 
